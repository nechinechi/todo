
<html>
  <head>
    <meta name="Content-Type" content="text/html; charset=UTF-8" />
    <title>Task Manager</title>
  </head>

  <body>

    <table border="1">
      <tr><th>todo</th><th>line</th><th>expire</th></tr>
      <% @res.each do |e| %>
      <tr>
	<td><%= e['todo'] %></td>
	<td><%= e['line'] %></td>
	<td><%= e['expire'] %></td>
      </tr>
      <% end %>
    </table>

  </body>
</html>
